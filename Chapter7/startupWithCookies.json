[
  {
    "description": "这是一个带cookie信息的请求",
    "request": {
      "uri": "/get/with/cookies",
      "method": "get"
    },
    "response": {
      "text": "这是一个需要携带cookies信息才能访问的get请求",
      "headers": {
        "Content-Type": "text/html;charset=gbk"
      }
    }

  },
  {
    "description": "这是一个带cookie的post请求",
    "request": {
      "uri":"/post/with/cookies",
      "method":"post",
      "cookies": {
        "login": "true"
      },
      "json": {
        "name": "huhansan",
        "age": "18"
      }
    },
    "response": {
      "status": 200,
      "json": {
        "huhansan": "success",
        "status": "1"
      },
      "headers": {
        "Content-Type": "text/html;charset=gbk"
      }
    }
  },
  {
    "description": "这是一个会返回cookie信息的get请求",
    "request": {
      "uri": "/getCookies",
      "method": "get"
    },
    "response": {
      "cookies": {
        "login": "true"
      },
      "text": "恭喜你获得cookie信息成功",
      "headers": {
        "Content-Type": "text/html;charset=gbk"
      }
    }
  }
]